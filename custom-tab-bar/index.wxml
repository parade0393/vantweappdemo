<!--custom-tab-bar/index.wxml-->
<van-tabbar active="{{ active }}" bind:change="onChange"  active-color="#333"
  inactive-color="#bbb" >
  <van-tabbar-item wx:for="{{list}}" wx:key="url" info="{{item.info}}">
    <image
      slot="icon"
      src="{{ item.normalIcon }}"
      mode="aspectFit"
      style="width: 30px; height: 18px;"
    />
    <image
      slot="icon-active"
      src="{{ item.selectIcon }}"
      mode="aspectFit"
      style="width: 30px; height: 18px;"
    />
    {{item.text}}
  </van-tabbar-item>
</van-tabbar>